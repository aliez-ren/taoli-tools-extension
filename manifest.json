{
	"manifest_version": 3,
	"name": "Taoli Tools Extension",
	"version": "0.0.1",
	"description": "Taoli Tools Extension",
	"permissions": ["declarativeNetRequest"],
	"host_permissions": [
		"https://taoli.tools/*",
		"http://localhost:5173/*",
		"*://*.gate.io/*",
		"*://*.gate.com/*",
		"*://*.gateio.ws/*",
		"*://*.bitget.com/*",
		"*://*.binance.com/*",
		"*://*.coinbase.com/*",
		"*://*.okx.com/*",
		"*://*.backpack.exchange/*",
		"*://*.apex.exchange/*",
		"*://*.asterdex.com/*",
		"*://*.bybit.com/*",
		"*://*.mexc.com/*"
	],
	"background": {
		"service_worker": "background.js"
	}
}
